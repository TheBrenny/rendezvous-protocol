    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "cli",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\cli.js",
            "console": "integratedTerminal",
            
            "args": [
                "${input:getRV}"
            ]
        }
    ],
    "inputs": [
        {
            "id": "getRV",
            "type": "promptString",
            "description": "Enter the Rendezvous URL",
        }
    ]
}